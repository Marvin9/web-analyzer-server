angeReport","generated":{"line":90,"column":0},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":2}},{"name":"dispatchChangeReport","generated":{"line":90,"column":2},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":2}},{"generated":{"line":90,"column":22},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":22}},{"name":"PropTypes","generated":{"line":90,"column":24},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":24}},{"name":"func","generated":{"line":90,"column":43},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":34}},{"generated":{"line":90,"column":47},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":24}},{"name":"isRequired","generated":{"line":90,"column":48},"source":"src/Components/seo-home/index.js","original":{"line":59,"column":39}},{"generated":{"line":91,"column":0},"source":"src/Components/seo-home/index.js","original":{"line":57,"column":18}},{"generated":{"line":91,"column":1},"source":"src/Components/seo-home/index.js","original":{"line":57,"column":0}}],"sources":{"src/Components/seo-home/index.js":"import React, { useState } from 'react';\nimport PropTypes from 'prop-types';\nimport GetURL from './Components/GetURL';\nimport Report from './Components/Report';\nimport apiURL from '../../../serverURL';\nimport Spinner from '../Spinner';\n\nexport default function index({ report, dispatchChangeReport }) {\n  const [loader, changeLoader] = useState(false);\n  const [error, changeError] = useState({ err: false, url: '' });\n\n  const api = apiURL;\n\n  const getReport = (url) => {\n    changeLoader(true);\n    changeError({ err: false, url: '' });\n\n    fetch(`${api}/analyze?url=${url}`)\n      .then((res) => res.json())\n      .then((generatedReport) => {\n        if (generatedReport.error) {\n          dispatchChangeReport([]);\n\n          changeError({\n            err: true,\n            url,\n          });\n        } else {\n          dispatchChangeReport([...generatedReport]);\n\n          setTimeout(() => {\n            window.scrollTo({\n              top: document.querySelector('#analyze-seo-form').scrollHeight,\n              behavior: 'smooth',\n            });\n          }, 100);\n        }\n      })\n      .catch((err) => {\n        throw err;\n      })\n      .finally(() => {\n        changeLoader(false);\n      });\n  };\n\n  return (\n    <>\n      <GetURL dispatchUrl={getReport} />\n      { loader\n        ? <Spinner />\n        : <Report report={report} error={error} /> }\n    </>\n  );\n}\n\nindex.propTypes = {\n  report: PropTypes.arrayOf(PropTypes.object).isRequired,\n  dispatchChangeReport: PropTypes.func.isRequired,\n};\n"},"lineCount":null}},"error":null,"hash":"3c20707657e9f0399308e01ebf08053a","cacheData":{"env":{}}}